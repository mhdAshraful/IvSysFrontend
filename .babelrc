{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime",
        [
            "babel-plugin-styled-components", 
            {
                "displayName": true,
                "fileName": true,
                 "meaninglessFileNames": ["index", "styles"]
            }
        ]
    ]
}
